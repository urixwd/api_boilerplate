    "eslint-watch": "^3.1.3",
    "glob": "^7.1.2",
    "mocha": "^4.0.1",
    "npm-run-all": "^4.1.2",
    "open": "0.0.5",
    "serverless-webpack": "^4.1.0",
    "supertest": "^3.0.0",
    "webpack": "^3.10.0",
    "webpack-node-externals": "^1.6.0"
