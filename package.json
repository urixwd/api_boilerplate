    "express": "^4.16.2",
    "express-validation": "^1.0.2",
    "joi": "^13.0.2",
    "morgan": "^1.9.0",
    "path": "^0.12.7",
    "rfs": "^5.0.1",
    "rotating-file-stream": "^1.3.4",
    "serverless-http": "^1.5.2"
  },
  "devDependencies": {
    "app-root-path": "^2.0.1",
    "babel": "^6.23.0",
